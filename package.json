{
  "name": "the-weasley-tracker",
  "type": "module",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "author": "The Weasley Tracker",
  "homepage": "https://electron-vite.org",
  "main": "./out/main/index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:cache": "eslint --cache .",
    "lint:fix": "eslint . --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm typecheck:node && pnpm typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "pnpm typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm build && electron-builder --dir",
    "build:win": "pnpm build && electron-builder --win",
    "build:mac": "pnpm build && electron-builder --mac",
    "build:linux": "pnpm build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@floating-ui/vue": "^1.1.9",
    "@modelcontextprotocol/sdk": "^1.17.3",
    "@vueuse/core": "^13.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "electron-store": "^10.1.0",
    "electron-updater": "^6.3.9",
    "lucide-vue-next": "^0.542.0",
    "openai": "^5.16.0",
    "pinia": "^3.0.3",
    "reka-ui": "^2.5.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.7",
    "vue-router": "^4.5.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@egoist/tailwindcss-icons": "^1.9.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@iconify-json/carbon": "^1.2.13",
    "@iconify-json/fluent": "^1.2.29",
    "@iconify-json/material-symbols": "^1.2.33",
    "@tailwindcss/vite": "^4.1.12",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "electron": "^37.3.0",
    "electron-builder": "^25.1.8",
    "electron-debug": "^4.1.0",
    "electron-vite": "^4.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-better-tailwindcss": "^3.7.4",
    "eslint-plugin-vue": "^10.4.0",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.8.3",
    "vite": "^7.1.2",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4",
    "vue": "^3.5.18",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "@tailwindcss/oxide"
    ]
  }
}
